<plugin name='hibernate-filter' version='0.1.2' grailsVersion='1.1.1 &gt; *'>
  <author>Scott Burch</author>
  <authorEmail>scott@bulldoginfo.com</authorEmail>
  <title>Integrates Hibernate filtering into Grails</title>
  <description>\
Integrates Hibernate filtering into Grails
</description>
  <documentation>http://grails.org/HibernateFilter+Plugin</documentation>
  <resources>
    <resource>UrlMappings</resource>
    <resource>DataSource</resource>
    <resource>HibernateFilterFilters</resource>
  </resources>
  <dependencies>
    <plugin name='controllers' version='1.1.1' />
    <plugin name='hibernate' version='1.1.1' />
  </dependencies>
</plugin>